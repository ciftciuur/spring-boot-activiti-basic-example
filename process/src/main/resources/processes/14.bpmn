    <extensionElements />
    <laneSet id="kullaniciGruplari">
      <lane id="GelirServisi" name="Gelir Servisi">
        <flowNodeRef>imarDurumuHarcOdeme</flowNodeRef>
      </lane>
      <lane id="ImarSehircilik" name="İmar ve Şehircilik Müdürlüğü">
        <flowNodeRef>imarDurumuBasvuruFormu</flowNodeRef>
        <flowNodeRef>kararDurumuKontrol</flowNodeRef>
        <flowNodeRef>verilemezSMS</flowNodeRef>
        <flowNodeRef>bitir</flowNodeRef>
        <flowNodeRef>basvuruTipiKontrol</flowNodeRef>
        <flowNodeRef>imarDurumuDosyasi</flowNodeRef>
        <flowNodeRef>imarDurumuSMS</flowNodeRef>
      </lane>
      <lane id="KentliServisi" name="Kentli Servisi">
        <flowNodeRef>basla</flowNodeRef>
      </lane>
      <lane id="ebysServisi" name="Ebys Servisi">
        <flowNodeRef>imarDurumuImza</flowNodeRef>
        <flowNodeRef>imzaBeklemeHavuzu</flowNodeRef>
