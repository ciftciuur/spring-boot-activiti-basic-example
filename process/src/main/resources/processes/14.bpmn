    </laneSet>
    <userTask id="imarDurumuBasvuruFormu" name="Başvuru değerlendirilir" activiti:formKey="com.beyazweb.KYS.modules.imar.forms.FrmImarDurumuBasvuru" activiti:candidateGroups="İmar ve Şehircilik Müdürlüğü">
      <documentation>Ücretli ise harç hesaplanır, verilemez ise gerekçesi yazılır.</documentation>
      <incoming>Flow_0oj481s</incoming>
      <outgoing>Flow_03h4hwz</outgoing>
    </userTask>
    <exclusiveGateway id="kararDurumuKontrol" name="Verilebilir mi?">
      <incoming>Flow_03h4hwz</incoming>
      <outgoing>flowVerilebilir</outgoing>
      <outgoing>flowVerilemez</outgoing>
    </exclusiveGateway>
    <serviceTask id="verilemezSMS" name="Vatandaşa verilemez smsi gönderilir" activiti:expression="${com.beyazweb.KYS.modules.imar.servis.SmsServis.send(&#34;VERILEMEZ&#34;)}">
      <incoming>flowVerilemez</incoming>
      <outgoing>Flow_1ojf17k</outgoing>
    </serviceTask>
    <endEvent id="bitir" name="İşlem bitti&#10;&#10;">
      <incoming>Flow_150fegb</incoming>
      <incoming>Flow_1ojf17k</incoming>
    </endEvent>
    <exclusiveGateway id="basvuruTipiKontrol" name="Ücretli mi?">
      <incoming>flowVerilebilir</incoming>
      <outgoing>flowUcretli</outgoing>
      <outgoing>flowUcretsiz</outgoing>
    </exclusiveGateway>
    <userTask id="imarDurumuHarcOdeme" name="Harç ödenir" activiti:formKey="com.beyazweb.KYS.modules.imar.forms.FrmImarDurumuHarcOdeme" activiti:candidateGroups="Gelir Servisi">
      <incoming>flowUcretli</incoming>
      <outgoing>Flow_1i19rvk</outgoing>
    </userTask>
    <userTask id="imarDurumuDosyasi" name="Düzenlenen imar durumu forma eklenir EBYS servisine gönderilir" activiti:formKey="com.beyazweb.KYS.modules.imar.forms.FrmImarDurumuBasvuru" activiti:candidateGroups="İmar ve Şehircilik Müdürlüğü">
      <incoming>Flow_1i19rvk</incoming>
      <incoming>flowUcretsiz</incoming>
      <outgoing>Flow_0d764pf</outgoing>
    </userTask>
    <serviceTask id="imarDurumuSMS" name="Vatandaşa belgeniz hazır smsi gönderilir" activiti:expression="${com.beyazweb.KYS.modules.imar.servis.SmsServis.send(&#34;BELGEHAZIR&#34;)}">
      <incoming>Flow_0mrqcmu</incoming>
      <outgoing>Flow_150fegb</outgoing>
    </serviceTask>
    <serviceTask id="imarDurumuImza" name="Belge imza için ebys sistemine gönderilir" activiti:expression="${com.beyazweb.KYS.modules.imar.servis.EbysServis.send(basvuruId)}">
