    </sequenceFlow>
    <sequenceFlow id="Flow_03h4hwz" sourceRef="imarDurumuBasvuruFormu" targetRef="kararDurumuKontrol" />
    <sequenceFlow id="Flow_1i19rvk" sourceRef="imarDurumuHarcOdeme" targetRef="imarDurumuDosyasi" />
