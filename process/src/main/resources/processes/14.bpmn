      <outgoing>Flow_0mrqcmu</outgoing>
    </task>
    <sequenceFlow id="flowUcretsiz" name="Hayır" sourceRef="basvuruTipiKontrol" targetRef="imarDurumuDosyasi">
      <conditionExpression xsi:type="tFormalExpression">${ucretDurumu=="UCRETSIZ"}</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="Flow_1ojf17k" sourceRef="verilemezSMS" targetRef="bitir" />
